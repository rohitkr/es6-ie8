{
  "plugins": [
    ["transform-es3-member-expression-literals"],
    ["transform-es3-property-literals"],
    ["transform-proto-to-assign"],
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true
    }]
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 0.2%"]
      },
      "modules": false,
      "loose": true
    }]
  ]
}
